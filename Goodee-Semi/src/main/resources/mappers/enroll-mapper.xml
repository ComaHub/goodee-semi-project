<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.goodee.semi.mapper.EnrollMapper">
	<update id="updateEnrollByPetNo" parameterType="com.goodee.semi.dto.PetClass">
		UPDATE enroll
		SET enroll_status = 'N'
		<where>
			pet_no = #{petNo} AND course_no = #{courseNo}
		</where>
	</update>
</mapper>